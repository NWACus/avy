{
  "cli": {
    "version": ">= 3.7.2",
    "appVersionSource": "remote"
  },
  "build": {
    "main": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "main"
    },
    "simulator": {
      "distribution": "internal",
      "channel": "main",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "preview",
      "autoIncrement": true
    },
    "release": {
      "channel": "release",
      "autoIncrement": true
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "IOS_USER_ID",
        "appleTeamId": "IOS_TEAM_ID",
        "ascAppId": "IOS_APP_ID"
      },
      "android": {
        "track": "internal"
      }
    },
    "release": {
      "ios": {
        "appleId": "IOS_USER_ID",
        "appleTeamId": "IOS_TEAM_ID",
        "ascAppId": "IOS_APP_ID"
      }
    }
  }
}
